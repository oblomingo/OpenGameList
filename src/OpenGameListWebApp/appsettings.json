{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "StaticFiles": {
      "Headers": {
        "Cache-Control": "no-cache, no-store",
        "Pragma": "no-cache",
        "Getting" "Ready",
        "Expires": "-1"
      }
    }
  }
}
